{
  "name": "ufi-plus-monorepo",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm -C play dev",
    "build": "pnpm -r --filter ./packages/** run build",
    "build:types": "pnpm -r --filter ./packages/** run build:types",
    "lint": "eslint --ext .ts,.tsx,.vue .",
    "test": "vitest",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "release": "changeset version && pnpm i -w && changeset publish",
    "gen": "bash scripts/gen.sh"
  },
  "dependencies":{
    "@ufi-plus/utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.27.0",
    "@typescript-eslint/parser": "^8.8.1",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "prettier": "^3.3.3",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.6",
    "changesets": "^1.0.2"
  }
}