{
  "name": "ufi-plus",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@ufi-plus/components": "workspace:*",
    "@ufi-plus/theme-chalk": "workspace:*"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
    "build:types": "vue-tsc -p tsconfig.json --declaration --emitDeclarationOnly"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "vue": "^3.5.12"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  }
}